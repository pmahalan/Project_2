{{!-- Divider with date --}}
<div class="container">
  <div class="divider"></div>
  <div class="section">
    {{!-- dynamically insert the date/event that was searched for --}}
    <h5>{{event.title}}</h5>
    <p>Mobile Keyword: {{event.keyword}}</p>
  </div>
  {{!-- Insert button to save as csv --}}

  <div class="row">
    <table class="responsive-table striped column">
      <thead>
        <tr>
          <th>Name of Attendee</th>
          <th>Email</th>
          <th>Phone</th>
        </tr>
      </thead>
      <tbody>
        {{#each person}}
        <tr>
          <td>{{dataValues.first_name}} {{dataValues.last_name}}</td>
          <td>{{dataValues.email}}</td>
          <td>{{dataValues.cell}}</td>
        </tr>
        {{/each}}
      </tbody>
  </div>
</div>